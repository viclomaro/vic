<h4>Nuevo proyecto</h4>
<form action="/proyectos/create" method="POST">
    <div class="form-group">
        <label>Titulo</label>
        <input type="text" name="titulo" id="" class="form-control">
    </div>
    <div class="form-group">
        <label>Descripci√≥n</label>
        <input type="text" name="descripcion" id="" class="form-control">
    </div>
    <div class="form-group">
        <label>Url</label>
        <input type="text" name="url" id="" class="form-control">
    </div>
    <div class="form-group">
        <label>Cliente</label>
        <input type="text" name="cliente" id="" class="form-control">
    </div>
    <div class="form-group">
        <label>Url Cliente</label>
        <input type="text" name="url_cliente" id="" class="form-control">
    </div>
    <div class="form-group">
        <label>Categoria</label>
        <select name="categoria" class="form-control">
            <option value="angular">Angular</option>
            <option value="react">React</option>
            <option value="nodejs">NodeJs</option>
            <option value="frontedn">frontend</option>
            <option value="backend">backend</option>
        </select>
    </div>
    <input type="submit" value="enviar" class="btn btn-block btn-primary">
</form>